<!doctype html>
<!--[if IE 7]>    <html class="ie7 oldie" lang="en" xmlns:fb="http://ogp.me/ns/fb#"> <![endif]-->
<!--[if IE 8]>    <html class="ie8 oldie" lang="en" xmlns:fb="http://ogp.me/ns/fb#"> <![endif]-->
<!--[if gt IE 8]> <!--> <html lang="en"> <!--<![endif]-->
    <head>
        <link rel="stylesheet" href="//fonts.googleapis.com/css?family=Ubuntu+Condensed|Open+Sans:400,700" />

        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <title>
            {{store_name}}
            {{#list_page}}{{#navigation}}{{#is_current}}- {{label}}{{/is_current}}{{/navigation}}{{/list_page}}
            {{#product_page}}{{#product}}- {{title}}{{/product}}{{/product_page}}
            {{#about_page}}- About Us{{/about_page}}
            {{#why-buy_page}}- Why Buy{{/why-buy_page}}
            {{#easy-returns_page}}- Easy Returns{{/easy-returns_page}}
            {{#contact_page}}- Contact Us{{/contact_page}}
        </title>

        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
        <meta name=viewport content="width=device-width,initial-scale=1,maximum-scale=1" />

        <!-- TODO: Add favicon -->

        <!-- TODO: Correct asset path {{assets_url}} -->
        <link href="//dhskp7m6mg2zv.cloudfront.net/theme_assets/shared/css/base.css" rel="stylesheet" />
        <link rel="stylesheet" href="/style.css" media="all" />

        <!--[if lt IE 9]>
            <script src="{{assets_url}}/shared/js/html5shiv.js"></script>
        <![endif]-->

        <!-- TODO: Add GA -->
    </head>
    <body>
        <div class="header--top">
           <a href="/">{{store_name}}</a>
        </div>

        {{#no_current_navigation}}
            <header role="banner">
                <a href="/"><img src="/images/lxls-logo.png" /></a>
            </header>
        {{/no_current_navigation}}

        <nav role="navigation">
            <ul class="navigation">
                {{#navigation}}
                    <li class="{{#is_current}}current{{/is_current}}">
                        <a href="{{url}}">{{label}}</a>
                    </li>
                {{/navigation}}
                <li{{#why-buy_page}} class="current"{{/why-buy_page}}>
                    <a href="/why-buy">Why Buy</a>
                </li>
                <li{{#easy-returns_page}} class="current"{{/easy-returns_page}}>
                    <a href="/easy-returns">Easy Returns</a>
                </li>
            </ul>
            {{#navigation}}{{#is_current}}{{#children?}}
                <ul class="navigation--children">
                    {{#children}}
                        <li class="{{#is_current}}current{{/is_current}}">
                            <a href="{{url}}">{{label}}</a>
                        </li>
                    {{/children}}
                </ul>
            {{/children?}}{{/is_current}}{{/navigation}}
        </nav>

        <div role="main">


            {{#no_current_navigation}}
                frontpage..
            {{/no_current_navigation}}


            {{#list_page}}{{^on_index}}
                <section class="products"><!--

                    {{#products}}
                     --><article>
                            <a href="{{url}}">
                                <div class="product__image">
                                    {{#primary_image}}
                                        <img src="{{url-500}}" alt="{{title}}">
                                    {{/primary_image}}
                                    {{#out_of_stock}}
                                        <span>Sold Out</span>
                                    {{/out_of_stock}}
                                </div>

                                <h2>{{title}}</h2>
                                <p></p>
                                {{#in_stock}}
                                    <div class="price">{{price_with_currency}} <del></del></div>
                                {{/in_stock}}
                                <div class="product__description">{{{description}}}</div>
                            </a>
                        </article><!--
                    {{/products}}

             --></section>
            {{/on_index}}{{/list_page}}


            {{#product_page}}

                {{#product}}
                    <article class="product" itemscope itemtype="http://schema.org/Product">
                        <div class="product__image">
                            {{#primary_image}}
                                <a href="{{url-1000}}">
                                    <img src="{{url-500}}" alt="{{title}}">
                                </a>
                            {{/primary_image}}
                        </div>

                        <section class="product__info">
                            <h1 itemprop="name" class="product__title">{{title}}</h1>
                            <div itemprop="description" class="product__description">
                                {{{description}}}
                            </div>

                            <div class="price" itemprop="offers" itemscope itemtype="http://schema.org/Offer">
                                <span itemprop="price">{{price_with_currency}}</span>
                                <del></del>

                                <meta itemprop="priceCurrency" content="{{currency_code}}">
                                {{#in_stock}}<link itemprop="availability" href="http://schema.org/InStock">{{/in_stock}}
                                {{#out_of_stock}}<link itemprop="availability" href="http://schema.org/OutOfStock">{{/out_of_stock}}
                            </div>

                            {{#add_to_cart}}
                                {{#variations_select}}{{/variations_select}}
                                {{#add_to_cart_button}}Grab this One{{/add_to_cart_button}}
                            {{/add_to_cart}}

                            {{#out_of_stock}}
                                <a class="button"
                                    href="mailto:stockholm@lxls.se?subject=Im%20interested%20in&body={{absolute_url}}">
                                    Request Item
                                </a>
                            {{/out_of_stock}}

                        </section><!--

                     --><figure class="product__media">
                            {{#all_images}}
                                <a href="{{url-1000}}">
                                    <img{{#is_primary}} class="product__image--primary"{{/is_primary}} src="{{url-500}}">
                                </a>
                            {{/all_images}}
                        </figure>
                    </article>
                {{/product}}

                <aside role="complementary">
                    {{#related_products}}
                        <h2>{{title}}<h2>
                    {{/related_products}}
                    <!-- TODO: Add related products -->

                    <nav class="breadcrumb">
                        <!-- TODO: JavaScript breadcrumbs -->
                    </nav>
                </aside>

            {{/product_page}}


            {{#about_page}}

                <article class="page">
                    <h1>About</h1>

                    <p><img src="http://lxls.se.preview.binero.se/drop/imgs/about-hangers.jpg" alt="About - LXLS Luxury Outlet" /></p>
                    <p>LXLS Luxury Outlet carry an exclusive range of hand-picked items from the world's most prestigious fashion houses at greatly reduced prices. The range is aimed for fashion conscious women and contain carefully selected clothing, shoes and accessories from previous seasons at a discount of 50-80%.</p>
                    <p>Emphasis is placed on the unique selection and exclusivity in small quantities in each style but we continue to add new goods to make the selection wider along the way.</p>
                    <p>We are a handful of people with extensive experience and roots within the international fashion business. Our mission is to offer the fashion conscious woman a unique selection of exclusive garments to affordable prices without intermediaries, presented in a tasteful manner.</p>

                    {{#store_email}}
                        {{#lang}}Contact:{{/lang}} <a href="mailto:{{store_email}}">{{store_email}}</a>
                    {{/store_email}}
                </article>

            {{/about_page}}

            {{#contact_page}}

                <article class="page html_content">
                    <h1>Contact Us</h1>

                    <p><a href="http://goo.gl/maps/0qOnx" target="_blank"><img src="http://lxls.se.preview.binero.se/drop/imgs/contact-map.jpg" alt="LXLS Luxury Outlet - Contact"></a></p>

                </article>

                <h2>Office</h2>
                <p>
                    LXLS Luxury Outlet<br>
                    Sofiagatan 3<br>
                    11640 Stockholm
                </p>
                <p>
                    E-mail: <a href="mailto:johanna@lxls.se?Subject=Customer Love">stockholm@lxls.se</a><br />
                    Phone: <a href="tel:+46101992595">+46 (0)10 199 25 95</a>
                </p>
                <p>Monday - Friday 10.00 – 20.00</p>
            {{/contact_page}}


            {{#why-buy_page}}

                <article class="page">
                    <h1>Why Buy from LXLS</h1>

                    <p><img src="http://lxls.se.preview.binero.se/drop/imgs/why-buy.jpg" alt="LXLS Luxury Outlet Why Buy"></p>
                    <p>STORES – LXLS Luxury Outlet buys big volume of luxury clothes from High-End shops around Europe. All clothes are from previous seasons, that is why we always can offer heavily discounted-% prices.</p>
                    <p>FACTORIES – LXLS Collection is produced directly from factories/suppliers without any middle-hand. That is why we always can offer heavily discounted-% prices.</p>
                </article>

            {{/why-buy_page}}


            {{#easy-returns_page}}

                <article class="page html_content">
                    <h1>Easy Returns to LXLS</h1>

                    <p><img src="https://dl.dropboxusercontent.com/u/2216038/lxls/returns.png" alt=""></p>
                    <ol>
                        <li>Do you feel like you have bought the wrong size, or the wrong item?</li>
                        <li>Call us or send us an email, and we will help you <a href="tel:+46101992595">+46 (0)10 199 25 95</a>, <a href="mailto:stockholm@lxls.se">stockholm@lxls.se</a></li>
                        <li>Send the item back to our office as a business package (företagspaket), Sofiagatan 3, 116 40 Stockholm.</li>
                        <li>As soon as we have received the item you will get your refund and a confirmation email.</li>
                    </ol>
                </article>

            {{/easy-returns_page}}


        </div>

        <footer role="contentinfo">
            <ul>
                <li><a href="/about">About</a></li>
                <li><a class="tictail_terms fullscreen fullscreen_iframe"
                    href="/legal/terms">Terms</a></li>
                <li><a class="tictail_return-policy fullscreen fullscreen_iframe"
                    href="/legal/return-policy">Returns</a></li>
                <li><a href="/contact">Contact</a></li>
            </ul>
        </footer>

        {{#product_page}}
            <div class="image__zoom" style="display:none"></div>
        {{/product_page}}

        <script src="//ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js"></script>
        <script src="/js/scripts.js"></script>

        <!-- TODO: Remove -->
        <iframe name="tt_colophon" id="tt_colophon" src="https://tictail.com/colophon/{{store_subdomain}}" style="position: fixed; z-index: 9999; top: 0px; right: 0px; height: 33px; overflow: hidden; background-image: none; border: 0px; width: 219px; background-position: initial initial; background-repeat: initial initial;"></iframe>
        <script>
            var __ttBaseUrl = 'https://tictail.com',
                __ttColophonUrl = 'https://tictail.com/colophon/lxls',
                __ttSearchUrl = '/search',
                __ttAssetsUrl = '{{assets_url}}',
                __ttLocaleData = {},
                __ttSubdomain = '{{store_subdomain}}',
                __ttAnalyticsId = 'UA-26369943-12';
        </script>
        <script src="//dmb7ixdwya1nh.cloudfront.net/static/js/v1/storefront/json.js"></script>
        <script src="//dmb7ixdwya1nh.cloudfront.net/static/js/storefront/mustache.js"></script>
        <script src="//dmb7ixdwya1nh.cloudfront.net/static/js/storefront/store.js"></script>
        <script>TT.store.domReady();</script>
    </body>
</html>